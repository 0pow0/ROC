namespace ROC;

table ActionInfo {
  uav_id: string;
  latitude: string;
  longitude: string;
}

table CreationInfo {
    uav_id: string;
    latitude: string;
    longitude: string;
    master_id: int;
}

table DeletionInfo {
   uav_id: string; 
}

union ROCType {
    Action: ActionInfo,
    Create: CreationInfo,
    Delete: DeletionInfo
}

table ROCInfo {
    info: ROCType;
}

root_type ROCInfo;
